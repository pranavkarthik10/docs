{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Flux",
  "colors": {
    "primary": "#6366F1",
    "light": "#818CF8",
    "dark": "#4F46E5"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/sdk",
              "guides/webhooks",
              "guides/events",
              "guides/authentication"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Events",
            "pages": [
              "api-reference/events/list",
              "api-reference/events/create",
              "api-reference/events/get"
            ]
          },
          {
            "group": "Webhooks",
            "pages": [
              "api-reference/webhooks/list",
              "api-reference/webhooks/create",
              "api-reference/webhooks/delete"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/flux-events",
          "icon": "github"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/flux",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Status",
        "href": "https://status.flux.dev"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.flux.dev"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/fluxdev",
      "github": "https://github.com/flux-events",
      "linkedin": "https://linkedin.com/company/fluxdev"
    }
  }
}
